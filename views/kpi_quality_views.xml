<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_kpi_quality_tree" model="ir.ui.view">
    <field name="name">payroll.kpi_quality_profile.tree</field>
    <field name="model">payroll.kpi_quality_profile</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="coef_ontime"/>
        <field name="coef_late"/>
        <field name="coef_overdue"/>
        <field name="active"/>
      </tree>
    </field>
  </record>

  <record id="view_kpi_quality_form" model="ir.ui.view">
    <field name="name">payroll.kpi_quality_profile.form</field>
    <field name="model">payroll.kpi_quality_profile</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="coef_ontime"/>
            <field name="coef_late"/>
            <field name="coef_overdue"/>
            <field name="active"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>

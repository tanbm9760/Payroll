<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_kpi_label_tree" model="ir.ui.view">
    <field name="name">payroll.kpi_label.tree</field>
    <field name="model">payroll.kpi_label</field>
    <field name="arch" type="xml">
      <tree>
        <field name="group_id"/>
        <field name="name"/>
        <field name="tag_id"/>
        <field name="weight"/>
        <field name="active"/>
      </tree>
    </field>
  </record>

  <record id="view_kpi_label_form" model="ir.ui.view">
    <field name="name">payroll.kpi_label.form</field>
    <field name="model">payroll.kpi_label</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="group_id"/>
            <field name="name"/>
            <field name="tag_id"/>
            <field name="weight"/>
            <field name="active"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
